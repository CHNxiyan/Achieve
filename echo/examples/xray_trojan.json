                "streamSettings": {
                    "network": "tcp",
                    "security": "tls",
                    "tlsSettings": {
                        "alpn": [
                            "h2",
                            "http/1.1"
                        ]
                    }
                }
            }
        ],
        "outbounds": [
            {
                "protocol": "freedom",
