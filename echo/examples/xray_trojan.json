                "settings": {
                    "clients": [
                        {
                            "password": "123456xx",
                            "email": "1.com"
                        }
                    ],
                    "network": "tcp,udp",
                    "fallbacks": [
                        {
                            "dest": "127.0.0.1:1234"
                        }
                    ]
